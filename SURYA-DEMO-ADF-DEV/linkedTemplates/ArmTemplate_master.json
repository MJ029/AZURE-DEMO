{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "SURYA-DEMO-ADF-DEV"
		},
		"LS_BLOB_USING_ACCOUNT_KEY_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_BLOB_USING_ACCOUNT_KEY'"
		},
		"LS_AZURE_KV_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "@{linkedService().ls_prm_kv_name}"
		},
		"LS_BLOB_USING_AKV_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "@linkedService().ls_prm_conn_string_name"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "SURYA-DEMO-ADF-DEV_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS_BLOB_USING_ACCOUNT_KEY_connectionString": {
						"value": "[parameters('LS_BLOB_USING_ACCOUNT_KEY_connectionString')]"
					},
					"LS_AZURE_KV_properties_typeProperties_baseUrl": {
						"value": "[parameters('LS_AZURE_KV_properties_typeProperties_baseUrl')]"
					},
					"LS_BLOB_USING_AKV_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('LS_BLOB_USING_AKV_properties_typeProperties_connectionString_secretName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}